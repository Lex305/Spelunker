{
  "schemaVersion": 1,
  "id": "spelunker",
  "version": "${version}",

  "name": "Spelunker",
  "description": "Adds the spelunker potion from Terraria",
  "authors": [
    "Leximon",
    "RasmusTV"
  ],
  "contributors": [
    "Loki0500 (Textures)",
    "Smato (French Translation)",
    "Rad233 (Chinese Translation)"
  ],
  "contact": {
    "sources": "https://github.com/Lex305/Spelunker",
    "issues": "https://github.com/Leximon/Spelunker/issues"
  },

  "license": "MIT",
  "icon": "assets/spelunker/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "de.leximon.spelunker.SpelunkerMod"
    ],
    "client": [
